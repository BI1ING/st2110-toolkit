# Configurations for LIST Web Server
# Use this file for development mode

# Filesystem related
folder: ~
cpp: ../../build/bin/

# HTTP-related
cookieSecret: STRD4O
webappDomain: http://localhost:8080

capture:
  interfaceName: eth0

# MongoDB database
database:
  hostname: localhost
  port: 27017

# InfluxDB URL
influx:
  hostname: localhost
  port: 8086

# RabbitMQ
rabbitmq:
  hostname: localhost
  port: 5672
